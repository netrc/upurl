{
	"manifest_version": 2,
	"name": "Up Url",
	"version": "1.0",
	"description": "enables auto-parsing and navigation of current url",
	"browser_action": {
		"default_icon": "icon.png",
		"default_popup": "upurl.html"
	},
	"icons": { 
		"48": "icon48.png",
		"128": "icon128.png"
	},
	"permissions": [
		"tabs"
	]
}

